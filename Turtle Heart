import turtle
import math
from turtle import*
def hearta(k):
    return 9*math.sin(k)**3
def heartb(K):
    return 6*math.cos(K)-5*\
  			math.cos(2*K)-2*\
  			math.cos(3*K)-\
  			math.cos(4*K)
speed(1000000)
turtle.Screen().bgcolor("black")
for i in range(6000):
  goto(hearta(i)*19,heartb(i)*19)
  print(i)
  for j in range(5):
     color("#730612")
  goto(0,0)
done()
